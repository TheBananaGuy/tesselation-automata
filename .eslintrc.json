{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["react-app", "eslint:recommended", "plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "no-console": "warn",
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "trailingComma": "es5",
        "semi": true,
        "singleQuote": true,
        "tabWidth": 2,
        "endOfLine": "crlf"
      }
    ]
  }
}
